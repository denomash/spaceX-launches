    "express-graphql": "^0.9.0",
    "graphql": "^14.5.8",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "nodemon": "^1.19.3"
  }
